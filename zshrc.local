			%W{pwd raw}.zip [PwdList, HistList], [PwdListKeys, HistListKeys], [] do |t, lst, lstKeys|
				item = nil
				if (ent = ENV["HOME"] / ".zsh_history_#{t}.d" / i.to_s)._e?
					item = t == "raw" ? ent.read.strip : ent.readline.strip
					if item.empty?
						item = nil
					end
				end
				Mutex.synchronize do
					lst[i] = item
					lstKeys.push i if item
				end
			end
		end
		$max.downto max_10 do |i|
			p i
			addEach[i]
		end
		p PwdList.size
		p PwdListKeys.size
		Thread.new do
			Thread.pass
			max_10.downto 1 do |i|
				addEach[i]
			end
		end
		p
		Thread.pass
	end
	def _listEach lst, hno
		start = lst.bsearch_index{ _1 <= hno }
		[start .. lst.size - 1, 0 ... start].each do |r|
			r.each do |i|
				yield lst[i]
			end
		end
	end
	def getDHist d_histno
		p
		d_histno = d_histno.to_i
		$d_histno = d_histno
		T.join
		p PwdList.size
		p PwdListKeys.size
		if PwdListKeys.empty?
			p
			return
		end
		p
		r2, w = IO.pipe
		r, w2 = IO.pipe
		pid = fork do
			p "/dev/tty"._e?
			p ENV["TTY"]
			r.close
			STDOUT.reopen w2
			w.close
			STDIN.reopen r2
			STDERR.reopen ENV["TTY"]
			if "/dev/tty"._e?
				Log.write_a "/dev/tty exist"
			else
				Log.write_a "/dev/tty do not exist"
			end
			"fzf".exec
			#"strace -f -o /home/yougain/log3 /home/yougain/.fzf/bin/fzf".exec
		end
		p "/dev/tty"._e?
		p ENV["TTY"]
		p pid
		p $max
		w2.close
		r2.close
		Thread.new do
			begin
				Thread.pass
				hash = {}
				[d_histno.downto(1), $max.downto(d_histno)].each do |r|
					r.each do |i|
						while true
							ks = nil
							Mutex.synchronize do
								ks = !PwdList.key?(i)
							end
							if ks
								sleep 0.1
								Thread.pass
							else
								break
							end
						end
						pwd = PwdList[i]
						if pwd && !hash.key?(pwd)
							hash[pwd] = 1
							p pwd
							w.write pwd.ln
							p
							w.flush
							p
						end
						p
					end
				end
				p
				w.close
				p
			rescue
				p $!
			end
			p
		end
		ret = r.read rescue ""
		p
		Process.waitpid pid
		p ret
		ret
	end
	$d_histno_arr = []
	$d_histno_arr_pos = 0
	$d_histno_hash = {}
	def dir_history mode, d_histno, histno, pwd
		if d_histno.empty?
			if mode == "backward"
				d_histno = histno.to_i
				$d_histno_arr.clear
				$d_histno_hash.clear
				$d_histno_hash[pwd] = true
				$d_histno_arr.push pwd
				$d_histno_arr_pos = 0
			else
				return pwd
			end
		else
			d_histno = d_histno.to_i
		end
		if mode == "backward"
			$d_histno_arr_pos += 1
			if $d_histno_arr[$d_histno_arr_pos]
				return PwdList[$d_histno_arr_pos]
			else
				[d_histno.downto(1), $max.downto(d_histno)].each do |r|
					r.each do |i|
						d = PwdList[i]
						if d && !$d_histno_hash[d]
							$d_histno_hash[pwd] = true
							$d_histno_arr.push pwd
							$d_histno_arr_pos = 0
							
						end
					end
				end
			end
		end
	end
'

