fnd(){
	local r
	if [[ $RAW_CMD_LINE =~ (^|\\W)fnd\\s+([^\|\ \t\<\>]+) ]];then
		r="${match[2]}"
		if [[ ${RAW_CMD_LINE:$MEND} =~ (^|\W)fnd\s ]];then
			echo "fnd: Error: multiple fnd found in the command line" >&2
			return 255
		fi
	fi
	if [ -n "$r" ];then
		local fn="${r##*/}"
		if [ "$fn" = "$r" ]; then
			find . -name "$r"
		else
			local dn="${r%/*}"
			if [ "$dn" = "~" ];then
				dn="$HOME"
			fi
			find "$dn" -name "$fn"
		fi
	else
		echo "fnd: Error: missing argument" >&2
		return 1
	fi
}


